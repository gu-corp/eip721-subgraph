  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gu-corp/eip721-subgraph.git",
    "directory": "packages/react"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@gu-corp/eip721-subgraph-client": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
