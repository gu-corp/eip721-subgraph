{
  "name": "@gu-corp/eip721-subgraph-client",
  "version": "0.2.0",
  "description": "TypeScript client for EIP-721 Subgraph with urql",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./queries": {
      "types": "./dist/queries/index.d.ts",
      "import": "./dist/queries/index.mjs",
      "require": "./dist/queries/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gu-corp/eip721-subgraph",
    "directory": "packages/client"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist src/types",
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@urql/core": "^5.0.0",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@urql/core": ">=4.0.0"
  }
}
